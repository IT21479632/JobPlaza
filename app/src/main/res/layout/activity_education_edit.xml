<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".candidate.Education_edit">

    <ScrollView
        android:id="@+id/secected_job_description"
        android:layout_width="wrap_content"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/content_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/page_secected_job_description_ek1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp">

                <View
                    android:id="@+id/rectangle_7"
                    android:layout_width="410dp"
                    android:layout_height="110dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/rectangle_7_shape"
                    android:backgroundTint="@color/top_box" />

                <TextView
                    android:id="@+id/software_engineer"
                    android:layout_width="214.5dp"
                    android:layout_height="39.5dp"
                    android:layout_marginLeft="36dp"
                    android:layout_marginTop="15dp"
                    android:text="Isuru Udantha"
                    android:textColor="@color/text_color"
                    android:textSize="23sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/Softwareisuru"
                    android:layout_width="321.5dp"
                    android:layout_height="28.5dp"
                    android:layout_marginLeft="36dp"
                    android:layout_marginTop="41dp"
                    android:text="Software Engineer"
                    android:textColor="@color/text_color"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/Experiences"
                    android:layout_width="110dp"
                    android:layout_height="39.5dp"
                    android:layout_marginLeft="155dp"
                    android:layout_marginTop="120dp"
                    android:text="Education"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/education_text"
                    android:textStyle="bold" />

                <View
                    android:id="@+id/rectangle_1"
                    android:layout_width="350dp"
                    android:layout_height="470dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="170dp"
                    android:background="@drawable/textbox"
                    android:elevation="10dp" />

                <TextView
                    android:id="@+id/we_are_seeking2"
                    android:layout_width="315dp"
                    android:layout_height="420dp"
                    android:layout_marginLeft="45dp"
                    android:layout_marginTop="190dp"
                    android:elevation="20dp"
                    android:text="@string/education"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_para" />

                <Button
                    android:id="@+id/save"
                    android:layout_width="100dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center_horizontal|center"
                    android:layout_marginLeft="160dp"
                    android:layout_marginTop="650dp"
                    android:backgroundTint="@color/button_color_blue"
                    android:gravity="center"
                    android:text="Save"
                    android:textColor="@color/white"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    app:cornerRadius="10dp" />

                <View
                    android:id="@+id/rectangle_9"
                    android:layout_width="410dp"
                    android:layout_height="81dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="720dp"
                    android:background="@drawable/rectangle_9_shape"
                    android:backgroundTint="@color/top_box" />

                <View
                    android:id="@+id/rectangle_10"
                    android:layout_width="59dp"
                    android:layout_height="50dp"
                    android:layout_marginStart="332dp"
                    android:layout_marginTop="735dp"
                    android:background="@drawable/rectangle_10_shape"
                    android:backgroundTint="@color/button_color_blue"
                    android:elevation="2dp" />


                <RelativeLayout
                    android:id="@+id/home"
                    class=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="51dp"
                    android:layout_marginTop="740dp">

                    <ImageView
                        android:id="@+id/vector_ek3"
                        android:layout_width="29.17dp"
                        android:layout_height="29.17dp"
                        android:layout_marginStart="2.92dp"
                        android:layout_marginTop="2.92dp"
                        android:contentDescription="vector_ek3"
                        android:src="@drawable/vector55" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/document"
                    class=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="148dp"
                    android:layout_marginTop="740dp">

                    <ImageView
                        android:id="@+id/vector"
                        android:layout_width="26.25dp"
                        android:layout_height="29.17dp"
                        android:layout_marginStart="4.38dp"
                        android:layout_marginTop="2.92dp"
                        android:contentDescription="vector"
                        android:src="@drawable/vector" />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/heart"
                    class=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="245dp"
                    android:layout_marginTop="740dp">

                    <ImageView
                        android:id="@+id/vector_ek1"
                        android:layout_width="29.17dp"
                        android:layout_height="26.25dp"
                        android:layout_marginStart="2.92dp"
                        android:layout_marginTop="4.38dp"
                        android:contentDescription="vector_ek1"
                        android:src="@drawable/vector_ek1" />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/user"
                    class=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="342dp"
                    android:layout_marginTop="740dp"
                    android:elevation="5dp">

                    <ImageView
                        android:id="@+id/vector_ek2"
                        android:layout_width="25.33dp"
                        android:layout_height="23.75dp"
                        android:layout_marginStart="6.33dp"
                        android:layout_marginTop="11.08dp"
                        android:contentDescription="vector_ek2"
                        android:elevation="5dp"
                        android:src="@drawable/vector_ek6" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/bell"
                    class=""
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="370dp"
                    android:layout_marginTop="15dp">

                    <ImageView
                        android:id="@+id/vector_ek4"
                        android:layout_width="18dp"
                        android:layout_height="15dp"
                        android:layout_marginStart="3dp"
                        android:layout_marginTop="2dp"
                        android:contentDescription="vector_ek4"
                        android:src="@drawable/vector_ek4" />

                    <ImageView
                        android:id="@+id/vector_ek5"
                        android:layout_width="4dp"
                        android:layout_height="2dp"
                        android:layout_marginStart="10.27dp"
                        android:layout_marginTop="21dp"
                        android:contentDescription="vector_ek5"
                        android:src="@drawable/vector_ek5" />

                    <View
                        android:id="@+id/ellipse_47"
                        android:layout_width="7.94dp"
                        android:layout_height="7.94dp"
                        android:layout_marginStart="13.73dp"
                        android:layout_marginTop="0.73dp"
                        android:background="@drawable/ellipse_47_shape"
                        android:backgroundTint="@color/button_color_red" />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/group_8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="130dp"
                    android:layout_marginTop="480dp">


                </RelativeLayout>


            </RelativeLayout>
        </RelativeLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>