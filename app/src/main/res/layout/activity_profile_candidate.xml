                    <View   android:id="@+id/rectangle_1_ek2"
                        android:background="@drawable/rectangle_1_ek2_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="340dp"
                        android:layout_height="146dp"
                        android:backgroundTint="#E1DFDF"/>

                    <TextView
                        android:id="@+id/proficient_in_programming_languages_such_as_java__python__or_c___knowledge_of_front_end_development_technologies"
                        android:layout_width="321.5dp"
                        android:layout_height="82.5dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="59dp"
                        android:text="@string/skills_udeshani"
                        android:textColor="@color/black"
                        android:textSize="15sp" />

                    <TextView android:id="@+id/skills"
                        android:textStyle="bold"
                        android:textSize="21sp"
                        android:textColor="@color/text_color"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="5dp"
                        android:layout_width="80.5dp"
                        android:layout_height="39.5dp"
                        android:text="@string/skills_string"
                        />




                </RelativeLayout>

                <RelativeLayout android:id="@+id/group_6"
                    android:layout_marginTop="633dp"
                    android:layout_marginStart="36dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <View   android:id="@+id/rectangle_1"
                        android:background="@drawable/rectangle_1_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="340dp"
                        android:layout_height="146dp"
                        android:backgroundTint="#E1DFDF" />

                    <TextView
                        android:id="@+id/bachelor_s_degree_in_computer_science_"
                        android:layout_width="321.5dp"
                        android:layout_height="28.5dp"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="59dp"
                        android:text="@string/education_udeshani"
                        android:textColor="@color/black"
                        android:textSize="15sp" />

                    <TextView android:id="@+id/education"
                        android:textStyle="bold"
                        android:textSize="21sp"
                        android:textColor="@color/text_color"
