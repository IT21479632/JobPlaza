<?xml version="1.0" encoding="utf-8"?>
<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".company.Profile_candidate">

    <ScrollView android:id="@+id/profile"
        android:layout_width="wrap_content"
        android:layout_height="match_parent" >

        <RelativeLayout android:id="@+id/content_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <RelativeLayout android:id="@+id/page_profile_ek1"
                android:layout_marginTop="0dp"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <ImageView android:id="@+id/image_33"
                    android:src="@drawable/image_33"
                    android:contentDescription="image_33"
                    android:layout_marginStart="147dp"
                    android:layout_marginTop="127dp"
                    android:layout_width="97dp"
                    android:layout_height="102dp" />

                <TextView
                    android:id="@+id/udeshani_name_and_email"
                    android:layout_width="201dp"
                    android:layout_height="62dp"
                    android:layout_gravity="center_horizontal|center"
                    android:layout_marginLeft="98dp"
                    android:layout_marginTop="245dp"
                    android:gravity="center"
                    android:text="@string/udeshani_name_and_email"
                    android:textColor="@color/black"
                    android:textSize="16sp" />


                <RelativeLayout android:id="@+id/group_4"
                    android:layout_marginTop="305dp"
                    android:layout_marginStart="36dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <View
                        android:id="@+id/rectangle_1_ek1"
                        android:layout_width="340dp"
                        android:layout_height="146dp"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
